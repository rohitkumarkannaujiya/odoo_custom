<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_property_report_action" model="ir.actions.report">
        <field name="name">Print Offer Report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="print_report_name">'estate property report'</field>
        <field name="binding_model_id" ref="model_estate_property"/>
    </record>
    <record id="user_property" model="ir.actions.report">
        <field name="name">Print User Property Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.user_property_offer_report</field>
        <field name="print_report_name">'User property report'</field>
        <field name="binding_model_id" ref="model_res_users"/>
    </record>
</odoo>